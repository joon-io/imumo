{
  "name": "imumo",
  "version": "1.1.0",
  "license": "MIT",
  "description": "Immutable Data Models for Javascript.",
  "keywords": [
    "immutable",
    "immutablejs",
    "object-oriented",
    "oo",
    "class",
    "extend"
  ],
  "repository": "https://github.com/joon-io/imumo",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "babel src -d dist",
    "build:bower": "webpack",
    "build:bower-prod": "NODE_ENV=production webpack",
    "lint": "eslint src tests",
    "lint:fix": "yarn run lint -- --fix",
    "test": "mocha tests --compilers js:babel-register",
    "test:dev": "yarn run test -- --watch"
  },
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-core": "^6.7.6",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.14.0",
    "chai": "^3.5.0",
    "eslint": "^3.5.0",
    "eslint-config-airbnb": "^11.0.0",
    "eslint-plugin-import": "^1.4.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.2.1",
    "mocha": "^3.0.2",
    "rimraf": "^2.5.2",
    "webpack": "^1.13.2"
  },
  "dependencies": {
    "babel-runtime": "^6.20.0",
    "immutable": "^3.8.1"
  },
  "author": "Joe Delgado"
